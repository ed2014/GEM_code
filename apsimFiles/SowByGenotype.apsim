                        days_after_harvest = 0
                        rotation_counter = rotation_counter + 1
                        HarvestFlag = 1
                        EndTheCrop = 0
                        EndByStage = 0
                        EndByStatus = 0
                        EndByDate = 0
                        EndBySilage = 0
                        ReadyToSow = 0  
                                         
endif  



</text>
              <event>end_of_day</event>
