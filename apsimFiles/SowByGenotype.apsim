   ETact = 0 ! actual plant evapotranspiration (mm)
   Tempsum = 0 ! Summing average temperature within a growth period
   PTtemp = 0 ! average temperature during (running) growth period

!---- Frost

frostDays = 0
frostIntensity = 0
